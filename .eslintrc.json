{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-undef": "off",
        "linebreak-style": "off",
        "class-methods-use-this": "off",
        "no-unused-vars": "off",
        "comma-dangle": "off",
        "arrow-parens": "off"
    }
}